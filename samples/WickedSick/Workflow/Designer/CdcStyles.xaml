<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="SlightSilver">#FFCCCCCC</Color>
    <Color x:Key="LightSilver">#FFEEEEEE</Color>
    <Color x:Key="LightSteelBlue">#FF81ABC7</Color>
    <Color x:Key="SteelBlue">#FF5684A5</Color>
    <Color x:Key="RoyalBlue">#FF256194</Color>
    <LinearGradientBrush x:Key="HeaderGradient" EndPoint="0,0" StartPoint="0,1">
        <GradientStop Color="#FF81ABC7" Offset="0"/>
        <GradientStop Color="#FF5684A5" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="SilverGradient" EndPoint="1,0.5" StartPoint="0,0.5">
        <GradientStop Color="#FFDDDDDD" Offset="0"/>
        <GradientStop Color="#FFDDDDDD" Offset="1"/>
        <GradientStop Color="White" Offset="0.5"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="SelectedMenuGradient" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#FFFEE17A" Offset="0"/>
        <GradientStop Color="#FFFFAB3F" Offset=".5"/>
        <GradientStop Color="#FFFEE17A" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="MenuGradient" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#FF81ABC7" Offset="0"/>
        <GradientStop Color="#FF5684A5" Offset=".5"/>
        <GradientStop Color="#FF81ABC7" Offset="1"/>
    </LinearGradientBrush>
</ResourceDictionary>