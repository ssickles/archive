<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="SDB.App"
	StartupUri="Window1.xaml">
	<Application.Resources>
		<!-- Resources scoped at the Application level should be defined here. -->
		<LinearGradientBrush x:Key="MenuGradient" EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="#FFCFDADE" Offset="0"/>
			<GradientStop Color="#FFA1BECD" Offset="1"/>
		</LinearGradientBrush>
		<SolidColorBrush x:Key="DarkBlue" Color="#FF003653"/>
		<SolidColorBrush x:Key="LightBlue" Color="#FFCFDADE"/>
		<SolidColorBrush x:Key="MediumBlue" Color="#FFA1BECD"/>
    </Application.Resources>
</Application>