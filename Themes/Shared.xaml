<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="DarkBlue">#1D384C</Color>
    <Color x:Key="MediumBlue">#DADADA</Color>
    <Color x:Key="LightGray">#F0F0F0</Color>
    <Color x:Key="LightMediumGray">#D4D4D4</Color>
    <Color x:Key="MediumGray">#A2A2A2</Color>
    <Color x:Key="DarkGray">#47484C</Color>
    <Color x:Key="MediumCobalt">#AFBCCD</Color>
    <Color x:Key="LightCobalt">#DCE1EA</Color>
    <Color x:Key="CobaltText">#576C8B</Color>

    <SolidColorBrush x:Key="DarkBlueBrush" Color="{StaticResource DarkBlue}"/>
    <SolidColorBrush x:Key="MediumBlueBrush" Color="{StaticResource MediumBlue}"/>
    <SolidColorBrush x:Key="LightGrayBrush" Color="{StaticResource LightGray}"/>
    <SolidColorBrush x:Key="LightMediumGrayBrush" Color="{StaticResource LightMediumGray}"/>
    <SolidColorBrush x:Key="MediumGrayBrush" Color="{StaticResource MediumGray}"/>
    <SolidColorBrush x:Key="DarkGrayBrush" Color="{StaticResource DarkGray}"/>
    <SolidColorBrush x:Key="MediumCobaltBrush" Color="{StaticResource MediumCobalt}"/>
    <SolidColorBrush x:Key="LightCobaltBrush" Color="{StaticResource LightCobalt}"/>
    <SolidColorBrush x:Key="CobaltTextBrush" Color="{StaticResource CobaltText}"/>

    <LinearGradientBrush x:Key="DarkBlueGradient" StartPoint=".5, 0" EndPoint=".5, 1">
        <GradientStop Color="#4C6B83" Offset="0"/>
        <GradientStop Color="#152D3F" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CobaltGradient" StartPoint=".5, 0" EndPoint=".5, 1">
        <GradientStop Color="{StaticResource LightCobalt}" Offset="0"/>
        <GradientStop Color="{StaticResource MediumCobalt}" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="WhiteGradient" StartPoint=".5, 0" EndPoint=".5, 1">
        <GradientStop Color="#FFFFFF" Offset="0"/>
        <GradientStop Color="#20FFFFFF" Offset=".4"/>
        <GradientStop Color="Transparent" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GreenGradient" StartPoint=".5, 0" EndPoint=".5, 1">
        <GradientStop Color="Green" Offset="0"/>
        <GradientStop Color="DarkGreen" Offset="1"/>
    </LinearGradientBrush>

    <ImageSource x:Key="IdsIconImage">Images/Regular/IDS.ico</ImageSource>
    <ImageSource x:Key="IdsAppLauncherIconImage">Images/Regular/IDS04.ico</ImageSource>
    <ImageSource x:Key="IdsMCIconImage">Images/Regular/IDS_MC01.ico</ImageSource>

    <ImageSource x:Key="IdsLogo">Images/Regular/idslogo.png</ImageSource>
    
